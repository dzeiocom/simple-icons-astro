---
import type { Props } from './index.d.ts'

const {
	xmlns = "http://www.w3.org/2000/svg",
	width,
	height,
	fill = "currentColor",
	viewBox = "0 0 24 24",
	size = 24,
	...rest
} = Astro.props;
---

<svg
	{xmlns}
	width={width ?? size}
	height={height ?? size}
	{fill}
	{viewBox}
	{...rest}
>
	<slot />
</svg>