---
import type { Props as IconProps } from './index.d.ts'

export interface Props extends IconProps {}

const {
	size = 24,
	width = size,
	height = size,
	fill = 'currentColor',
	viewBox = '0 0 24 24'
} = (Astro.props ?? {})
---

<svg
	xmlns="http://www.w3.org/2000/svg"
	class={Astro.props.class}
	width={width}
	height={height}
	fill={fill}
	viewBox={viewBox}
>
	<slot />
</svg>